<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" onMouseDragEntered="#ViewBooks" onMouseEntered="#AnchorPaneEntered" prefHeight="800.0" prefWidth="800.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="e.book.store.DashBoardController">
    <stylesheets>
        <URL value="@dashboard.css" />
    </stylesheets>
   <children>
      <Button fx:id="AdminAdd" layoutX="647.0" layoutY="32.0" mnemonicParsing="false" onAction="#OnAddButtonClicked" prefHeight="28.0" prefWidth="112.0" styleClass="Buttons" text="Add" textFill="WHITE" visible="false">
         <font>
            <Font name="Century Gothic" size="13.0" />
         </font>
      </Button>
      <Button layoutX="45.0" layoutY="33.0" mnemonicParsing="false" onAction="#backOnClick" styleClass="Buttons" text="Back" textFill="#fffefe">
         <font>
            <Font name="Century Gothic" size="14.0" />
         </font>
      </Button>
      <TilePane layoutX="45.0" layoutY="139.0" prefHeight="504.0" prefWidth="719.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <ImageView fx:id="Book1" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#OnClick" pickOnBounds="true" preserveRatio="true" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <ImageView fx:id="Book2" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#OnClick" pickOnBounds="true" preserveRatio="true" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <ImageView fx:id="Book3" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#OnClick" pickOnBounds="true" preserveRatio="true" />
               </children>
            </VBox>
            <VBox prefHeight="158.0" prefWidth="121.0">
               <children>
                  <ImageView fx:id="Book4" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#OnClick" pickOnBounds="true" preserveRatio="true" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <ImageView fx:id="Book5" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#OnClick" pickOnBounds="true" preserveRatio="true" />
               </children>
            </VBox>
            <VBox onMouseClicked="#OnClick" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <ImageView fx:id="Book6" fitHeight="150.0" fitWidth="137.0" onMouseClicked="#OnClick" pickOnBounds="true" preserveRatio="true" />
               </children>
            </VBox>
            <VBox prefHeight="171.0" prefWidth="178.0">
               <children>
                  <ImageView fx:id="Book7" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#OnClick" pickOnBounds="true" preserveRatio="true" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <ImageView fx:id="Book8" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#OnClick" pickOnBounds="true" preserveRatio="true" />
               </children>
            </VBox>
         </children>
      </TilePane>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="276.0" layoutY="31.0" text="Book Dashboard" textFill="WHITE">
         <font>
            <Font name="Century Gothic" size="24.0" />
         </font>
      </Label>
      <Button fx:id="FavouriteList" layoutX="640.0" layoutY="33.0" mnemonicParsing="false" onAction="#OnFavButtonClicked" styleClass="Buttons" text="Favourite List" textFill="WHITE" visible="false">
         <font>
            <Font name="Century Gothic" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
