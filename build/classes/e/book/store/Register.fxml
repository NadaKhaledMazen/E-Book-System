<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="e.book.store.RegisterController">
    <stylesheets>
        <URL value="@register.css" />
    </stylesheets>
   <children>
      <TextField fx:id="UserNameSignUp" layoutX="31.0" layoutY="240.0" prefHeight="26.0" prefWidth="234.0" promptText="Username" />
      <TextField fx:id="AnswerSignUp" layoutX="31.0" layoutY="425.0" prefHeight="26.0" prefWidth="234.0" promptText="Answer" />
      <PasswordField fx:id="PasswordSignUp" layoutX="31.0" layoutY="308.0" prefHeight="26.0" prefWidth="234.0" promptText="password" />
      <Label layoutX="59.0" layoutY="135.0" prefHeight="47.0" prefWidth="178.0" text="Register Form" textFill="WHITE">
         <font>
            <Font name="Century Gothic" size="25.0" />
         </font>
      </Label>
      <Button layoutX="430.0" layoutY="491.0" mnemonicParsing="false" onAction="#SignUpOnClick" styleClass="Buttons" text="Sign Up" textFill="WHITE">
         <font>
            <Font name="Century Gothic Bold" size="18.0" />
         </font></Button>
      <Button layoutX="14.0" layoutY="547.0" mnemonicParsing="false" onAction="#BackOnClick" prefHeight="26.0" prefWidth="117.0" styleClass="Buttons" text="Back" textFill="WHITE">
         <font>
            <Font name="Century Gothic" size="14.0" />
         </font></Button>
      <Label layoutX="31.0" layoutY="373.0" text="What is your Favorite Food?" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
